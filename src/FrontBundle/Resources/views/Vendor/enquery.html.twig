{% if app.user and app.user.couple and enquiryForm is not null  %}

{% form_theme enquiryForm 'FrontBundle::Vendor/fields.html.twig' %}

{{ form_start(enquiryForm, {'action': path('vendor_profile', {'id':vendorService.id})}) }}

  {{ form_row(enquiryForm.name) }}
  {{ form_row(enquiryForm.email) }}
  {{ form_row(enquiryForm.phone) }}
  {{ form_row(enquiryForm.weddingDate) }}
  <div class="send-me">{% trans %}Send Me Info Via{% endtrans %}</div>
  {{ form_row(enquiryForm.emailResponseBack) }}
  {{ form_row(enquiryForm.phoneCallBack) }}
  <button type="button" class="btn btn-pink">{% trans %}Send enquery{% endtrans %}</button>
  {{ form_widget(enquiryForm._token) }}
{{ form_end(enquiryForm) }}
{% else %}
<br>
<h6>{% trans %}You must be connected to send enquery{% endtrans %}</h6>
<br>
  <a href="#" data-toggle="modal" data-target="#login-model">{% trans %}Sign In{% endtrans %}</a>
{% endif %}