{% block title %}SubscriptionRequest{% endblock %}

{% block body %}
    <h1>SubscriptionRequest</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ subscription_request.id }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ subscription_request.user }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ subscription_request.phone }}</td>
            </tr>
            <tr>
                <th>EmailAddress</th>
                <td>{{ subscription_request.emailAddress }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ subscription_request.createdAt ? subscription_request.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

